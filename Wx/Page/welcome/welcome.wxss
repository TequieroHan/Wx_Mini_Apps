/* Page/welcome/welcome.wxss */
page{
  width: 100%;
  height: 100%;
  background: #b3d4db;
 
}
/**
* align-items 所有元素居中
*/
.contianer{
  display: flex;
  flex-direction: column;
  align-items:center; 
}
/**
*头像
*/
image{
  margin-top: 100rpx;
  width: 200rpx;
  height: 200rpx;
}
.user_name{
  margin-top: 100rpx;
  font-weight: bold;
  margin-bottom: 150rpx;
}
text{
  font-size: 32rpx;
  font-family: MicroSoft Yahei;
}
.btn_welcome {
  padding: 15rpx 30rpx 15rpx 30rpx;
  border-color: #405f80;
  border-radius: 10rpx;
  border-style: solid;
  border-width: 1rpx;
  color: #405f80;
}